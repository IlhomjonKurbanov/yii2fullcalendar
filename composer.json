{
	"name": "philippfrenzel/yii2fullcalendar",
	"description": "Yii2 fullcalendar Widgets",
	"keywords": ["yii2", "fullcalendar","widget"],
	"homepage": "http://www.frenzel.net/",
	"type": "yii2-extension",
	"license": "MIT",
	"authors": [
		{
			"name": "Philipp Frenzel",
			"email": "philipp@frenzel.net",
			"homepage": "http://www.frenzel.net/",
			"role": "Developer"
		}
	],
	"support": {
		"source": "https://github.com/philippfrenzel/yii2fullcalendar"
	},
	"minimum-stability": "stable",
	"require": {
		"yiisoft/yii2" : "*",
		"yiisoft/yii2-jui": "*",
		"bower-asset/punycode": "^1.3.0 !=1.3.2",
        	"bower-asset/jquery": "^1.12 !=1.12.4",
		"bower-asset/fullcalendar" : "*"
	},
	"require-dev": {
		"codeception/codeception": "*"
	},
	"autoload": {
		"psr-4": { "yii2fullcalendar\\": "" }
	},
	"config": {
		"fxp-asset": {
			"installer-paths": {
				"bower-asset-library": "vendor/bower"
			},
			"ignore-files": {
				"bower-asset/fullcalendar": false
			}
		}
	}
}
